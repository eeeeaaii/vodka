| testname: 
| description: test isword against data type

~(_is-word ~(_quote ~(_car _)_)_)
~(_is-word !no_)
~(_is-word ~(_quote @a_)_)
~(_is-word #0_)
~(_is-word $"a"_)
~(_is-word %0_)
~(_is-word [nil]_)
~(_is-word &(__)_)
~(_is-word ~(_quote &(__)_)_)
~(_is-word [letter]"a"_)
~(_is-word [separator]";"_)
~(_is-word [word](__)_)
~(_is-word (||)_)
~(_is-word [doc](||)_)
~(_is-word [line](__)_)
~(_is-word ~(_convert-type-if-error $"warn" ~(_car _)_)_)


