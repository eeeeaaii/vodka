| testname: 
| description: test is-nil against diff data types

~(_is-nil ~(_quote ~(_car _)_)_)
~(_is-nil !no_)
~(_is-nil ~(_quote @a_)_)
~(_is-nil #0_)
~(_is-nil $"a"_)
~(_is-nil %0_)
~(_is-nil [nil]]_)
~(_is-nil &(__)_)
~(_is-nil ~(_quote &(__)_)_)
~(_is-nil [letter]"a"_)
~(_is-nil [separator]";"_)
~(_is-nil [word](__)_)
~(_is-nil (||)_)
~(_is-nil [doc](||)_)
~(_is-nil [line](__)_)
~(_is-nil ~(_convert-type-if-error $"warn" ~(_car _)_)_)


